@{
  Layout = "_Layout";
}
<div>
  <nav class="navbar navbar-expand-lg navbar-light">
  <a class="navbar-brand" href="#">Pierre's</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item">
        <a class="nav-link" href="/">Home<span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/vendors">Vendors</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Orders</a>
      </li>
  </div>
</div>
<div class="container">
  <div class="jumbotron">
    <h1>Vendor Order Details: </h1>

    <h2>@Model["orders"].Title</h2>
    <h2>@Model["orders"].Description</h2>
    <h2>@Model["orders"].Price</h2>
    <h2>@Model["orders"].Date</h2>
    <h3>Vendor: @Model["vendors"].Name</h3>

    <a href='/vendors/@Model["vendors"].Id/orders/new'>Add another order</a>
    <a href='/vendors'>View all vendors</a>
  </div>
</div>