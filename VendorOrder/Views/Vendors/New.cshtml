@{
  Layout = "_Layout";
}
<div class="container">
  <h1>Add a Vendor</h1>

  <form action="/vendors" method="post">
    <label for="vendorName">Vendor name</label>
    <input id="vendorName" name="vendorName" type="text">
    <label for="vendorDescription">Vendor description</label>
    <input for="vendorDescription" name="vendorDescription" type="text">
    <button class="btn btn-primary" type="submit">Add</button>
  </form>

  <p><a href="/">Back home</a></p>
  <p><a href="/vendors">Back to categories</a></p>
</div>